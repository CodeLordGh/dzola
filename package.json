{
  "name": "vscode-test-generation",
  "displayName": "Test Generation",
  "description": "AI-powered test generation for VS Code",
  "version": "0.1.0",
  "publisher": "baboh-kossi-mawuena",
  "author": {
    "name": "Baboh Kossi Mawuena",
    "email": "baboh.kossi@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Testing",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "test",
    "generator",
    "ai",
    "automation",
    "tdd"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "test-generator.generateTest",
        "title": "Generate Test"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config webpack.config.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src --ext ts",
    "dev": "nodemon --watch src --ext ts --exec \"npm run compile\""
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^3.7.7",
    "@types/estree": "^1.0.6",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.11.5",
    "@types/semver": "^7.5.8",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "minimatch": "^9.0.3"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "@anthropic-ai/sdk": "^0.12.2",
    "@azure/openai": "^1.0.0-beta.11",
    "@google-cloud/vertexai": "^0.2.1",
    "openai": "^4.24.7"
  }
}
