{
  "name": "dzola",
  "displayName": "Dz…îla - AI Test Generator",
  "description": "An open-source AI-powered test generation extension for VS Code, named after the Ewe word for 'guardian'",
  "version": "0.0.1",
  "publisher": "baboh-kossi-mawuena",
  "author": {
    "name": "Baboh Kossi Mawuena",
    "email": "baboh.kossi@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Testing",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "test",
    "generator",
    "ai",
    "automation",
    "tdd"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "test-generator.generateTest",
        "title": "Generate Test"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config webpack.config.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src --ext ts",
    "dev": "nodemon --watch src --ext ts --exec \"npm run compile\""
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^3.7.7",
    "@types/estree": "^1.0.6",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^16.18.34",
    "@types/semver": "^7.5.8",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "minimatch": "^9.0.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
